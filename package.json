    "laravel-echo": "^1.4.0",
    "laravel-echo-ratchet": "^1.4.3",
    "popper": "^1.0.1",
    "popper.js": "^1.14.4",
    "pusher-js": "^4.3.0",
    "socket.io": "^2.1.1",
